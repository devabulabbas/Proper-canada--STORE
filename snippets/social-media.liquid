{{ 'social-media.css' | asset_url | stylesheet_tag }}

{% liquid 
  assign show_heading = false
  if settings.instagram_link != blank or settings.twitter_link != blank or settings.tiktok_link != blank or settings.snapchat_link != blank or settings.pinterest_link != blank or settings.tumblr_link != blank or settings.facebook_link != blank or settings.youtube_link != blank or settings.vimeo_link != blank or settings.reddit_link != blank or settings.trustpilot_link != blank or settings.tripadvisor_link != blank or settings.telegram_link != blank
    assign show_heading = true
  endif
%}

{% style %}
    #shopify-section-{{ section.id }} {;
        --social-media-vertical-alignment: {{ social-media_settings.social_media_vertical_alignment }};
        --social-media-horizontal-alignment: {{ social-media_settings.social_media_horizontal_alignment }};
    }
  
    .social-media__icon {
        height: {{ 24 | times: settings.desktop_font_scale | divided_by: 100.0 | round | at_least: 24 }}px;
        aspect-ratio: 1/1;
    }
    @media screen and (max-width: 920px) {
        .social-media__icon {
            height: {{ 24 | times: settings.mobile_font_scale | divided_by: 100.0 | round | at_least: 24 }}px;
        }
    }
{% endstyle %}

<div class="social-media-block
            social-media-block--{{ section.settings.grid_columns }} 
            social-media-block--{{ social-media_settings.block_width }} 
            social-media-block--{{ social-media_settings.social_media_horizontal_alignment }} 
            {% if section.id contains 'header' %}social-media-block--header{% elsif section.id contains 'footer' %}social-media-block--footer{% else %}social-media-block--{{ section.id }}{% endif %}"
            style="--social-media-vertical-alignment: {{ social-media_settings.social_media_vertical_alignment }}; --social-media-horizontal-alignment: {{ social-media_settings.social_media_horizontal_alignment }}; --item-index: {{ for_loop.index }}" {{ block_item.shopify_attributes }}> 
    <div class="social-media-container
                social-media-container--{{ social-media_settings.social_media_style }} 
                social-media-container--{{ settings.white_space }}
                {% if section.id contains 'header' %}social-media-container--header{% elsif section.id contains 'footer' %}social-media-container--footer{% else %}social-media-container--{{ section.id }}{% endif %}
                social-media-container--{{ social-media_settings.social_media_horizontal_alignment }} 
