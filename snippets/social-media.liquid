                        if social-media_settings.social_media_style == 'logo_in_circle'
                        assign social_media_name = 'instagram-icon'
                        assign social_media_file_name = social_media_name | handle | append: '.' | append: file_extension
                        assign social_image = social_media_file_name | file_img_url | prepend: 'https:' | split: '?' | first
                        endif
                    -%}
