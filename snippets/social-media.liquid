                        {% if social-media_settings.social_media_style == 'logo_in_circle' and images[social_media_file_name] != blank %}style="--social-media-icon: url({{ social_image }})"{% endif %}>
                        {%- if social-media_settings.social_media_style == 'text' -%}
                            <span class="social-media__text">{{ 'general.social_media.text.instagram' | t }}</span>
                        {%- elsif social-media_settings.social_media_style == 'short_text' -%}
                            <span class="social-media__short-text">{{ 'general.social_media.short_text.instagram' | t }}</span>
