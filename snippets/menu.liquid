                                                                {{ 'mega-menu.css' | asset_url | stylesheet_tag }}
                                                        {%- endif -%}
                                                        {% if section.id contains 'main-sidebar' and menu_settings.menu_style == 'slide_out' %}<div class="slide-out-container">{% endif %}
                                                                <ul class="menu__dropdown-child">
                                                                        {% for child_link in link.links %}
