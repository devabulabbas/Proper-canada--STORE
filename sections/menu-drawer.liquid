{{ 'menu-drawer.css' | asset_url | stylesheet_tag }}
{{ 'menus.css' | asset_url | stylesheet_tag }}

{% style %}
#shopify-section-{{ section.id }} {
    --grid-columns: {{ section.settings.grid_columns }};
    --left-and-right-paddings: {{ section.settings.left_and_right_paddings }}px;
    {% if section.settings.color_type == 'invert' %}
      --layout-text-color:
        {{ settings.layout_background_color.red }},
        {{ settings.layout_background_color.green }},
        {{ settings.layout_background_color.blue }};
      --layout-background-color:
        {{ settings.layout_text_color.red }},
        {{ settings.layout_text_color.green }}, 
        {{ settings.layout_text_color.blue }};
      {% endif %}
      {% if section.settings.color_type == 'custom' %}
      --layout-text-color:
        {{ section.settings.text_color.red }},
        {{ section.settings.text_color.green }}, 
        {{ section.settings.text_color.blue }};
      --layout-background-color:
        {{ section.settings.background_color.red }},
        {{ section.settings.background_color.green }}, 
        {{ section.settings.background_color.blue }};
    {% endif %}
}
{% endstyle %}


{%- liquid
    assign blocks = section.blocks 
    assign animationDuration = 0
    if settings.animation_duration == '0.3'
        assign animationDuration = 'basic'
    elsif settings.animation_duration == '0.5'
        assign animationDuration = 'sophisticated'
    endif
  -%}

  {%- liquid
    if settings.typography_preset == 'avant_garde'
            assign close_weight = settings.accent_icon_weight
            if settings.swap_fonts
                    assign close_weight = settings.base_icon_weight
            endif
    else
            assign close_weight =  settings.base_icon_weight
            if settings.swap_fonts
                    assign close_weight = settings.accent_icon_weight
            endif
    endif
-%}
<menu-drawer>
<div class="menu-drawer hidden color-{{ section.settings.color_type }} {% if section.settings.color_type != 'default' %}no-default-color{% endif %}" id="menu-drawer" hidden>
    <div class="menu-drawer__content 
                menu-drawer__content--{{ settings.white_space }} 
                menu-drawer__content--{{ section.settings.grid_columns }}
                menu-drawer__content--{{ animationDuration }}">
        {%- for block in blocks -%}
            {%- case block.type -%}
                {%- when 'text' -%}
                    {%- render 'text', text_settings: block.settings, type: block.type, block_item: block, for_loop: forloop -%}
                {%- when 'search' -%}
                    {%- render 'search', search_settings: block.settings, type: block.type, block_item: block, for_loop: forloop -%}
                {%- when 'main_menu' -%}
                    {% if block.settings.menu != blank %}
                        {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu, block_item: block, type: block.type, for_loop: forloop -%}
                    {% endif %}
                {%- when 'secondary_menu' -%}
                    {% if block.settings.menu_1 != blank or block.settings.menu_2 != blank or block.settings.menu_3 != blank or block.settings.menu_4 != blank or block.settings.menu_5 != blank %}
                        <div class="block-{{ forloop.index }} menus-block {{ block.settings.menu_vertical_alignment }} menus-block--{{ section.settings.grid_columns }} menus-block--{{ block.settings.block_width }} menus-block--{{ block.settings.menu_alignment }}" style="--menus-vertical-alignment: {{ block.settings.menu_vertical_alignment }}; --menus-horizontal-alignment: {{ block.settings.menu_horizontal_alignment }}; --item-index: {{ forloop.index }};" {{ block.shopify_attributes }}>
                            {%- liquid 
                                assign menu_item_1 = 0 | times: 1
                                assign menu_item_2 = 0 | times: 1
                                assign menu_item_3 = 0 | times: 1
                                assign menu_item_4 = 0 | times: 1
                                assign menu_item_5 = 0 | times: 1
                                if block.settings.menu_1 != blank
                                assign menu_item_1 = 1 | times: 1
                                endif
                                if block.settings.menu_2 != blank 
                                assign menu_item_2 = 1 | times: 1
                                endif
                                if block.settings.menu_3 != blank 
                                assign menu_item_3 = 1 | times: 1
                                endif
                                if block.settings.menu_4 != blank 
                                assign menu_item_4 = 1 | times: 1
                                endif
                                if block.settings.menu_5 != blank 
                                assign menu_item_5 = 1 | times: 1
                                endif
                                assign menus_items = menu_item_1 | plus: menu_item_2 | plus: menu_item_3 | plus: menu_item_4 | plus: menu_item_5
                            -%}
                            <div class="menus menus--{{ block.settings.menu_horizontal_alignment }} menus--{{ block.settings.menu_view }}" style="--menus-items: {{ menus_items }}">
                                {% if block.settings.menu_1 != blank %}
                                    <div class="menus__item menus__item--{{ settings.white_space }}">
                                        {% if block.settings.menu_1_heading != blank %}
                                            <p class="menus__item-title menus__item-title--{{ settings.typography_preset }} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.menu_1_heading }}</p>
                                        {% endif %}
                                        {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu_1, block_item: block, type: block.type -%}
                                    </div>
                                {% endif %}
                                {% if block.settings.menu_2 != blank %}
                                    <div class="menus__item menus__item--{{ settings.white_space }}">
                                        {% if block.settings.menu_2_heading != blank %}
                                            <p class="menus__item-title menus__item-title--{{ settings.typography_preset }} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.menu_2_heading }}</p>
                                        {% endif %}
                                        {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu_2, block_item: block, type: block.type -%}
                                    </div>
                                {% endif %}
                                {% if block.settings.menu_3 != blank %}
                                    <div class="menus__item menus__item--{{ settings.white_space }}">
                                        {% if block.settings.menu_3_heading != blank %}
                                            <p class="menus__item-title menus__item-title--{{ settings.typography_preset }} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.menu_3_heading }}</p>
                                        {% endif %}
                                        {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu_3, block_item: block, type: block.type -%}
                                    </div>
                                {% endif %}
                                {% if block.settings.menu_4 != blank %}
                                    <div class="menus__item menus__item--{{ settings.white_space }}">
                                        {% if block.settings.menu_4_heading != blank %}
                                            <p class="menus__item-title menus__item-title--{{ settings.typography_preset }} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.menu_4_heading }}</p>
                                        {% endif %}
                                        {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu_4, block_item: block, type: block.type -%}
                                    </div>
                                {% endif %}
                                {% if block.settings.menu_5 != blank %}
                                    <div class="menus__item menus__item--{{ settings.white_space }}">
                                        {% if block.settings.menu_5_heading != blank %}
                                            <p class="menus__item-title menus__item-title--{{ settings.typography_preset }} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.menu_5_heading }}</p>
                                        {% endif %}
                                        {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu_5, block_item: block, type: block.type -%}
                                    </div>
                                {% endif %}
                            </div>
                        </div>
                    {% endif %}
                {%- when 'miscellaneous' -%}
                    {%- render 'miscellaneous', miscellaneous_settings: block.settings, type: block.type, block: block, for_loop: forloop -%}
                {%- when 'line' -%}
                    <div class="block-{{ forloop.index }} line-container line-container--{{ section.settings.grid_columns }} line-container--{{ block.settings.block_width }} line-container--{{ section.id }}" data-block-index="{{ forloop.index0 }}" {{ block.shopify_attributes }} style="--line-alignment: {{ block.settings.line_alignment }}; --item-index: {{ forloop.index }};">
                        <span class="line {% if section.settings.color_type == 'custom' %}color-custom{% elsif section.settings.color_type == 'invert' %}color-invert{% endif %}"></span>
                    </div>
                {%- when 'newsletter' -%}
                    {%- render 'newsletter', newsletter_settings: block.settings, block_item: block, for_loop: forloop -%}
                {%- when 'social_media' -%}
                    {%- render 'social-media', social-media_settings: block.settings, type: block.type, block_item: block, for_loop: forloop, section_settings: section.settings -%}
                {%- when 'image' -%}
                    {%- render 'image', image_settings: block.settings, type: block.type, block_item: block, for_loop: forloop -%}
                {%- when 'empty_space' -%}
                    <div class="block-{{ forloop.index }} empty-space empty-space--{{ section.settings.grid_columns }} empty-space--{{ block.settings.block_width }}" {{ block.shopify_attributes }} style="--item-index: {{ forloop.index }}"></div>
                {%- when 'country_and_language' -%}
                    {%- render 'country-and-language', country-and-language_settings: block.settings, type: block.type, for_loop: forloop, block_item: block -%}
                {%- when 'contacts' -%}
                    {%- render 'contacts', contacts_settings: block.settings, for_loop: forloop, block_item: block -%}
                {%- when 'context_image' -%}
                    <div class="context-image-container context-image-container--{{ section.settings.grid_columns }} context-image-container--{{ block.settings.block_width }} context-image-container--{{ block.id }}" style="--item-index: {{ forloop.index }};" {{ block.shopify_attributes }}>
                        {%- render 'context-image', block_or_section: block, block: block -%}
                    </div>
                {%- when 'buttons' -%}
                    <div class="navigation-buttons navigation-buttons--{{ block.id }} navigation-buttons--{{ section.settings.grid_columns }} navigation-buttons--{{ block.settings.block_width }}" data-block-index="{{ forloop.index0 }}" style="--item-index: {{ forloop.index }};" {{ block.shopify_attributes }}>
                        <div class="navigation-buttons__wrapper">
                            {%- render 'button-with-icon', button_settings: block.settings, button_label: block.settings.button_label_1, button_link: block.settings.button_link_1, button_style: block.settings.button_style_1, target_blank: block.settings.target_blank_1, icon: block.settings.icon_1, block: block -%}
                            {%- render 'button-with-icon', button_settings: block.settings, button_label: block.settings.button_label_2, button_link: block.settings.button_link_2, button_style: block.settings.button_style_2, target_blank: block.settings.target_blank_2, icon: block.settings.icon_2, block: block -%}
                        </div>
                    </div>
                {%- when 'custom_liquid' -%}
                    {%- render 'custom-liquid', block: block -%}
            {%- endcase -%}
        {%- endfor -%}
    </div>
    <div class="button-wrapper menu-drawer__close {% if settings.enable_buttons_zoom %}buttons-zoom{% endif %}">
        <button type="button" class="button-close">
          {% if close_weight == 'light' %}
              {% render 'icon', icon: 'cross-light' %}
          {% elsif close_weight == 'regular' %}
              {% render 'icon', icon: 'cross-regular' %}
          {% elsif close_weight == 'medium' %}
              {% render 'icon', icon: 'cross-medium' %}
          {% elsif close_weight == 'bold' %}
              {% render 'icon', icon: 'cross-bold' %}
          {% endif %}
        </button>
    </div>
</div>
</menu-drawer>

