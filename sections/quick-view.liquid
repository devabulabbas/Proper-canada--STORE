                                                  {% elsif weight == 'bold' %}
                                                      {% render 'icon-accordion', icon: 'arrow-right-bold' %}
                                                  {% endif %}
                                              </span>
                                          </a>
                                      </div>
                                  {% endif %}
                                {% endcase %}
