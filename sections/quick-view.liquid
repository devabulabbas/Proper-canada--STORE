                                            srcset="{% if media.preview_image.width >= 59 %}{{ media.preview_image | image_url: width: 59 }} 59w,{% endif %}
                                                    {% if media.preview_image.width >= 118 %}{{ media.preview_image | image_url: width: 118 }} 118w,{% endif %}
                                                    {% if media.preview_image.width >= 84 %}{{ media.preview_image | image_url: width: 84 }} 84w,{% endif %}
                                                    {% if media.preview_image.width >= 168 %}{{ media.preview_image | image_url: width: 168 }} 168w,{% endif %}
                                                    {% if media.preview_image.width >= 130 %}{{ media.preview_image | image_url: width: 130 }} 130w,{% endif %}
                                                    {% if media.preview_image.width >= 260 %}{{ media.preview_image | image_url: width: 260 }} 260w{% endif %}"
                                            src="{{ media | image_url: width: 84, height: 84 }}"
                                            sizes="(min-width: 1200px) calc((1200px - 19.5rem) / 12), (min-width: 750px) calc((100vw - 16.5rem) / 8), calc((100vw - 8rem) / 5)"
                                            alt="{{ media.alt | escape }}"
                                            height="200"
                                            width="200"
                                            class="product__image"
                                            loading="lazy"
                                            draggable="false"
                                            {% if settings.disable_right_click %}oncontextmenu="return false"{% endif %}
                                            >
                                        </button>
                                        </li>
                                    {%- endunless -%}
                                    {%- endfor -%}
                                </ul>
                                </div>
                                </slider-component>
                            
                            {%- endif -%}
                        </div>
                        </media-gallery>
                    </div>
                    <div class="product__info-wrapper {% if section.settings.swap_position %}product__info-wrapper--left{% endif %}">
                        <div class="product__info-block ">
                        <div id="ProductInfo-{{ section.id }}" class="product__info-container {% if section.settings.sticky_content and section.settings.desktop_gallery_style == 'one_column_grid' or section.settings.sticky_content and section.settings.desktop_gallery_style == 'two_columns_grid' and product.media.size <= 1 %} container--sticky{% endif %}">
                            {%- assign product_form_id = 'product-form-' | append: section.id -%}
        
                                {%- for block in section.blocks -%}
                                {%- case block.type -%}
                                {%- when '@app' -%}
                                <div class="app">{% render block %}</div>
                                {%- when 'subtitle' -%}
                                    {% if block.settings.subtitle != blank %}
                                    <{{ block.settings.subtitle_tag }} class="product__text product__text--{{ settings.typography_preset }} {% if block.settings.dim_text %}dim{% endif %} {% if block.settings.subtitle_size != 'default' %}{{ block.settings.subtitle_size }}{% endif %}" {{ block.shopify_attributes }}>
                                        {% if block.settings.subtitle == product.vendor %}
                                        {{ product.vendor | link_to_vendor : class: "link" }}
                                        {% else %}
                                        {{- block.settings.subtitle -}}
                                        {% endif %}
                                    </{{ block.settings.subtitle_tag }}>
                                    {% endif %}
                                {%- when 'title' -%}
                                    <{{ block.settings.heading_tag }} class="product__title {% if settings.accent_color_for_headings %}product__title--accent{% endif %} product__title--{{ settings.typography_preset }} {% if block.settings.title_size != 'default' %}{{ block.settings.title_size }}{% endif %}" {{ block.shopify_attributes }}>
                                    {{ product.title | escape }}
                                    </{{ block.settings.heading_tag }}>
                                {%- when 'sku' -%}
                                    {% if product.selected_or_first_available_variant.sku != blank %}
                                    <p class="product__sku body-secondary-text--{{ settings.typography_preset }} {% if block.settings.sku_size != 'default' %}{{ block.settings.sku_size }}{% endif %} {% if settings.dim_text %}dim{% endif %}" id="sku-{{ section.id }}" {{ block.shopify_attributes }}>
                                        {{ block.settings.sku_heading }} {{ product.selected_or_first_available_variant.sku }}
                                    </p>
                                    {% endif %}
                                {%- when 'text' -%}
                                  {% if block.settings.text != blank %}
                                    <div class="product__text product__text--{{ settings.typography_preset }} richtext {% if settings.enable_animated_underline %}richtext-link--underline{% endif %} {% if block.settings.dim_text %}dim{% endif %} {% if block.settings.text_size != 'default' %}{{ block.settings.text_size }}{% endif %}" {{ block.shopify_attributes }}>
                                        {{- block.settings.text -}}
                                    </div>
                                  {% endif %}
                                {%- when 'description' -%}
                                    <div class="product__description rte rte-block
                                    body-secondary-text--{{ settings.typography_preset }}
                                    {% if block.settings.text_size != 'default' %}{{ block.settings.text_size }}{% endif %}" {{ block.shopify_attributes }}>
                                    {{ product.description }}
                                    </div>
                                {%- when 'custom_liquid' -%}
                                    <div class="body-secondary-text--{{ settings.typography_preset }}">{{ block.settings.custom_liquid }}</div>
                                {%- when 'collapsible_tab' -%}
                                    {%- render 'accordion', accordion_settings: block.settings, page: block.settings.page, block_item: block -%}
                                {%- when 'complementary_products' -%}
                                    {%- render 'complementary-products', complementary_products_settings: block.settings, block_item: block, section_content: false -%}
                                {%- when 'share' -%}
                                    <div class="share" {{ block.shopify_attributes }}>
                                    {% if block.settings.display_collapsible_link %}
                                        {%- liquid 
                                        if settings.typography_preset == 'avant_garde'
                                            assign weight = settings.accent_icon_weight
                                            if settings.swap_fonts
                                                assign weight = settings.base_icon_weight
                                            endif
                                        else 
                                            assign weight =  settings.base_icon_weight
                                            if settings.swap_fonts
                                                assign weight = settings.accent_icon_weight
                                            endif
                                        endif
                                        -%}
                                        <accordion-block>
                                        <div class="accordion-toggle no-js-hidden share-toggle">
                                            <div class="accordion__summary" tabindex="0">
                                            <div class="summary__title share__button hover-opacity share__button--{{ settings.typography_preset }}">
                                                <div class="summary__title-name">
                                                <span class="share__title">{{ "products.product.share_button" | t }}</span>
                                                </div>
                                                <span class="icon-accordion share__icon">
                                                <span class="icon-accordion__close share__icon-close">
                                                    {% if weight_accordion_title == 'regular' %}
                                                    {% render 'icon-accordion-2', icon: 'share-regular' %}
                                                    {% elsif weight_accordion_title == 'medium' %}
                                                    {% render 'icon-accordion-2', icon: 'share-medium' %}
                                                    {% elsif weight_accordion_title == 'bold' %}
                                                    {% render 'icon-accordion-2', icon: 'share-bold' %}
                                                    {% endif %}
                                                </span>
                                                <span class="icon-accordion__open share__icon-open">
                                                    {% if weight_accordion_title == 'regular' %}
                                                    {% render 'icon', icon: 'cross-regular' %}
                                                    {% elsif weight_accordion_title == 'medium' %}
                                                    {% render 'icon', icon: 'cross-medium' %}
                                                    {% elsif weight_accordion_title == 'bold' %}
                                                    {% render 'icon', icon: 'cross-bold' %}
                                                    {% endif %}
                                                </span>
                                                </span>
                                            </div>
                                            </div>
                                            <div class="accordion__panel">
                                            <div class="accordion__content share-items">
                                                {%- liquid
                                                assign share_image = product.selected_or_first_available_variant.featured_media.preview_image | default: product.featured_media.preview_image
                                                render 'share', share_settings: block.settings, share_title: product.title, share_link: product.url, share_variant_url: product.selected_variant.url, share_image: share_image
                                                -%}
                                            </div>
                                            </div>
                                        </div>
                                        </accordion-block>
                                        <noscript>
                                        <details id="Details-{{ block.id }}-{{ section.id }}" class="share-toggle">
                                            <summary>
                                            <div class="share__button hover-opacity share__button--{{ settings.typography_preset }}">
                                                <span class="share__title">{{ "products.product.share_button" | t }}</span>
                                                <span class="share__icon">
                                                <span class="share__icon-close">
                                                    {% if weight == 'regular' %}
                                                        {% render 'icon-accordion-2', icon: 'share-regular' %}
                                                    {% elsif weight == 'medium' %}
                                                        {% render 'icon-accordion-2', icon: 'share-medium' %}
                                                    {% elsif weight == 'bold' %}
                                                        {% render 'icon-accordion-2', icon: 'share-bold' %}
                                                    {% endif %}
                                                </span>
                                                <span class="share__icon-open">
                                                    {% if weight == 'regular' %}
                                                        {% render 'icon', icon: 'cross-regular' %}
                                                    {% elsif weight == 'medium' %}
                                                        {% render 'icon', icon: 'cross-medium' %}
                                                    {% elsif weight == 'bold' %}
                                                        {% render 'icon', icon: 'cross-bold' %}
                                                    {% endif %}
                                                </span>
                                                </span>
                                            </div>
                                            </summary>
                                            <div class="share-items">
                                            {%- liquid
                                                assign share_image = product.selected_or_first_available_variant.featured_media.preview_image | default: product.featured_media.preview_image
                                                render 'share', share_settings: block.settings, share_title: product.title, share_link: product.url, share_variant_url: product.selected_variant.url, share_image: share_image
                                            -%}
                                            </div>
                                        </details>
                                        </noscript>
                                    {% else %}
                                        {%- liquid
                                        assign share_image = product.selected_or_first_available_variant.featured_media.preview_image | default: product.featured_media.preview_image
                                        render 'share', share_settings: block.settings, share_title: product.title, share_link: product.url, share_variant_url: product.selected_variant.url, share_image: share_image
                                        -%}
                                    {% endif %}
                                    </div>
                                {%- when 'rating' -%}
                                    {%- if product.metafields.reviews.rating.value != blank -%}
                                    <a class="rating-container rating-container--main-product rating-container--{{ settings.typography_preset }}" href="#" onclick="var el=document.querySelector('#shopify-product-reviews[data-id=\'{{ product.id }}\']');el&&el.scrollIntoView();return false;">
                                        {% liquid
                                        assign rating_decimal = 0
                                        assign decimal = product.metafields.reviews.rating.value.rating | modulo: 1
                                        assign rating_decimal = decimal
                                        %}
                                        <div class="rating" role="img" aria-label="{{ 'accessibility.star_reviews_info' | t: rating_value: product.metafields.reviews.rating.value, rating_max: product.metafields.reviews.rating.value.scale_max }}">
                                        <span aria-hidden="true" class="rating-star rating-star--{{ settings.typography_preset }}" style="--rating: {{ product.metafields.reviews.rating.value.rating | floor }}; --rating-max: {{ product.metafields.reviews.rating.value.scale_max }}; --rating-decimal: {{ rating_decimal }};"></span>
                                        </div>
                                        <p class="rating-text caption">
                                        <span aria-hidden="true">{{ product.metafields.reviews.rating.value | slice: 0, 3 }} / {{ product.metafields.reviews.rating.value.scale_max }}</span>
                                        </p>
                                        <p class="rating-count caption">
                                        <span aria-hidden="true">{{ product.metafields.reviews.rating_count }} {% if product.metafields.reviews.rating_count > 1 %}{{ "accessibility.reviews" | t }}{% else %}{{ "accessibility.review" | t }}{% endif %}</span>
                                        <span class="visually-hidden">{{ product.metafields.reviews.rating_count }} {{ "accessibility.total_reviews" | t }}</span>
                                        </p>
                                    </a>
                                    {%- endif -%}
                                {%- when 'related_collections' -%}
                                    {% if block.settings.heading != blank or block.settings.collection_by_brand or block.settings.shop_by_brand or block.settings.shop_the_collection %}
                                    {%- render 'product-related-collections', related_collections_settings: block.settings, block_item: block, section_content: false -%}
                                    {% endif %}
                                {%- when 'price' -%}
                                    <div class="no-js-hidden price-block" id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
                                    {%- render 'price', product: product, price_settings: block.settings, use_variant: true, show_badges: true, type: block.type -%}
                                    {%- if shop.shipping_policy.body != blank or cart.taxes_included -%}               
                                      <div class="product__shipping-policy richtext {% if settings.enable_animated_underline %}richtext-link--underline{% endif %} product__shipping-policy--{{ settings.typography_preset }} {% if settings.dim_text %}dim{% endif %}">
                                        {% if cart.taxes_included %}{{ 'products.product.include_taxes' | t }}&nbsp;{% endif %}{% if shop.shipping_policy.body != blank %}{{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}{% endif %}
                                      </div>
                                    {%- endif -%}
                                    <div class="product__payment-terms product__payment-terms--{{ settings.typography_preset }}">
                                        {%- form 'product', product, data-productid: product.id, id: 'product-form-installment', class: 'installment caption-large' -%}
                                        <input type="hidden" name="id" data-productid="{{ product.id }}" value="{{ product.selected_or_first_available_variant.id }}">
                                        {{ form | payment_terms }}
                                        {%- endform -%}
                                    </div>
                                    </div>
                                {%- when 'quantity_selector' -%}
                                    <div class="product-form__input product-form__quantity" {{ block.shopify_attributes }}>
                                    {% if block.settings.show_label %}
                                        <label class="form__label label--{{ settings.typography_preset }} {% if settings.dim_text %}dim{% endif %}" for="Quantity-{{ section.id }}">
                                        {{ 'products.product.quantity.label' | t }}
                                        </label>
                                    {% endif %}
                                    {% assign weight = settings.base_icon_weight %}
                                    <quantity-input class="quantity">
                                        <button class="quantity__button no-js-hidden" name="minus" type="button">
                                        <span class="visually-hidden">{{ 'products.product.quantity.decrease' | t: product: product.title | escape }}</span>
                                            {% if weight == 'regular' %}
                                                    {% render 'icon', icon: 'minus-regular' %}
                                            {% elsif weight == 'medium' %}
                                                    {% render 'icon', icon: 'minus-medium' %}
                                            {% elsif weight == 'bold' %}
                                                    {% render 'icon', icon: 'minus-bold' %}
                                            {% endif %}
                                        </button>
                                        <input class="quantity__input label--{{ settings.typography_preset }}"
                                            type="number"
                                            name="quantity"
                                            id="Quantity-{{ section.id }}"
                                            aria-label="{{ 'products.product.quantity.input_label' | t: product: product.title | escape }}"
                                            min="1"
                                            value="1"
                                            form="{{ product_form_id }}"
                                            onclick="this.select();"
                                        >
                                        <button class="quantity__button no-js-hidden" name="plus" type="button">
                                        <span class="visually-hidden">{{ 'products.product.quantity.increase' | t: product: product.title | escape }}</span>
                                            {% if weight == 'regular' %}
                                                    {% render 'icon', icon: 'plus-regular' %}
                                            {% elsif weight == 'medium' %}
                                                    {% render 'icon', icon: 'plus-medium' %}
                                            {% elsif weight == 'bold' %}
                                                    {% render 'icon', icon: 'plus-bold' %}
                                            {% endif %}
                                        </button>
                                    </quantity-input>
                                    </div>
                                {%- when 'variant_picker' -%}
                                  <div class="variants">
                                    {%- unless product.has_only_default_variant -%}
                                          {%- liquid 
                                            assign color_pills_variant_label = settings.color_pills_variant_label | downcase
                                            assign pills_variant_label = settings.pills_variant_label | downcase
                                            assign variant_names = color_pills_variant_label | append: pills_variant_label
                                          -%}
                                            <variant-selects class="no-js-hidden variant-selects variant-selects--{{ settings.variant_layout }} {% if settings.stretch_dropdowns %}variant-selects--stretched{% endif %}" style="--options-size: {{ product.options.size }};" data-section="{{ section.id }}" data-url="{{ product.url }}" {{ block.shopify_attributes }}>
                                              <span class="visually-hidden" style="--options-size: {{ product.options.size }};"></span>
                                              {% for option in product.options_with_values %}
                                                {% assign downcased_option = option.name | downcase %}
                                                {% if settings.variant_layout == 'column' and variant_names contains downcased_option %}
                                                  <variant-radios class="no-js-hidden" data-section="{{ section.id }}" data-url="{{ product.url }}">
                                                        <fieldset class="js product-form__input {% if settings.pills_layout != 'original' %}stretched{% endif %}">
                                                          {% if block.settings.show_label %}
                                                            <legend class="form__label label--{{ settings.typography_preset }} {% if settings.dim_text %}dim{% endif %}">{{ option.name }} </legend>
                                                          {% endif %}
                                                          <div class="form__variants">
                                                            {%- liquid
                                                              assign is_color = false
                                                              if settings.color_pills_variant_label != blank
                                                                assign swatch_file_extension = 'png'
                                                                assign swatch_trigger = 'products.product.color_swatch_tag' | t | downcase
                                                                
                                                                if downcased_option contains swatch_trigger
                                                                  assign is_color = true
                                                                elsif swatch_trigger == 'color' and downcased_option contains 'colour'
                                                                  assign is_color = true
                                                                endif
                                                              endif
                                        
                                                              assign option_idx = forloop.index
                                                            -%}
                                                            {%- liquid
                                                              assign variants_available_arr = product.variants | map: 'available'
                                                              assign variants_option1_arr = product.variants | map: 'option1'
                                                              assign variants_option2_arr = product.variants | map: 'option2'
                                                              assign variants_option3_arr = product.variants | map: 'option3'
                                                            
                                                              assign product_form_id = 'product-form-' | append: section.id
                                                            -%}
                                                            {%- for value in option.values -%}
                                                              {%- liquid
                                                                assign option_disabled = true
                                                            
                                                                for option1_name in variants_option1_arr
                                                                  case option.position
                                                                    when 1
                                                                      if variants_option1_arr[forloop.index0] == value and variants_available_arr[forloop.index0]
                                                                        assign option_disabled = false
                                                                      endif
                                                                    when 2
                                                                      if option1_name == product.selected_or_first_available_variant.option1 and variants_option2_arr[forloop.index0] == value and variants_available_arr[forloop.index0]
                                                                        assign option_disabled = false
                                                                      endif
                                                                    when 3
                                                                      if option1_name == product.selected_or_first_available_variant.option1 and variants_option2_arr[forloop.index0] == product.selected_or_first_available_variant.option2 and variants_option3_arr[forloop.index0] == value and variants_available_arr[forloop.index0]
                                                                        assign option_disabled = false
                                                                      endif
                                                                  endcase
                                                                endfor
                                                              -%}
                                                              <input type="radio" id="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}"
                                                                name="{{ option.name }}"
                                                                value="{{ value | escape }}"
                                                                form="{{ product_form_id }}"
                                                                {% if option.selected_value == value %}checked{% endif %}
                                                                {% if option_disabled %}
                                                                  class="disabled"
                                                                {% endif %}
                                                              >
              
                                                              {%- if is_color and color_pills_variant_label contains downcased_option -%}
                                                                {%- liquid
                                                                  assign color_file_name = value | handle | append: '.' | append: swatch_file_extension
                                                                  assign color_image = color_file_name | file_img_url: '50x50' | prepend: 'https:' | split: '?' | first
                                                                  assign color_swatch_fallback = value | split: ' ' | last | handle
                                                                -%}
                                                                <label
                                                                  id="Option-value-{{ section.id }}"
                                                                  for="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}"
                                                                  class="color__swatch input--{{ settings.typography_preset }} color__swatch--{{ settings.pills_layout }}"
                                                                  style="{% if images[color_file_name] != blank %}--swatch-background-image: url({{ color_image }});{% endif %} --swatch-background-color: {{ color_swatch_fallback }};"
                                                                >
                                                                  <span class="color__swatch-dot"></span>
                                                                  {% if settings.show_color_name == false %}
                                                                    <span class="color__swatch-tooltip color__swatch-tooltip--{{ settings.typography_preset }}">{{ value }}</span>
                                                                  {% else %}
                                                                    <span class="color__swatch-name">{{ value }}</span>
                                                                  {% endif %}
                                                                </label>
                                                              {%- else -%}
                                                                <label
                                                                  for="{{ section.id }}-{{ option.name }}-{{ forloop.index0 }}"
                                                                  id="Option-value-{{ section.id }}"
                                                                  class="color__swatch input--{{ settings.typography_preset }} color__swatch--{{ settings.pills_layout }}"
                                                                >
                                                                  {{ value }}
                                                                </label>
                                                              {%- endif -%}
                                                            {%- endfor -%}
                                                          </div>
                                                        </fieldset>
                                                    <script type="application/json">
                                                      {{ product.variants | json }}
                                                    </script>
                                                  </variant-radios>
                                                {% else %}
                                                  <div class="product-form__input product-form__input--dropdown {% if settings.stretch_dropdowns %}product-form__input--dropdown--stretched{% endif %}">
                                                    {% if block.settings.show_label %}
                                                      <label class="form__label label--{{ settings.typography_preset }} {% if settings.dim_text %}dim{% endif %}" for="Option-{{ section.id }}-{{ forloop.index0 }}">
                                                        
                                                        {{ option.name }}
                                                      </label>
                                                    {% endif %}
                                                    {%- liquid
                                                      assign variants_available_arr = product.variants | map: 'available'
                                                      assign variants_option1_arr = product.variants | map: 'option1'
                                                      assign variants_option2_arr = product.variants | map: 'option2'
                                                      assign variants_option3_arr = product.variants | map: 'option3'
                                                    
                                                      assign product_form_id = 'product-form-' | append: section.id
                                                    -%}
                                                    <div class="select">
                                                      <select id="Option-{{ section.id }}-{{ forloop.index0 }}"
                                                        class="select__select input--{{ settings.typography_preset }}"
                                                        name="options[{{ option.name | escape }}]"
                                                        form="{{ product_form_id }}"
                                                      >
                                                        {%- for value in option.values -%}
                                                          {%- liquid
                                                            assign option_disabled = true
                                                        
                                                            for option1_name in variants_option1_arr
                                                              case option.position
                                                                when 1
                                                                  if variants_option1_arr[forloop.index0] == value and variants_available_arr[forloop.index0]
                                                                    assign option_disabled = false
                                                                  endif
                                                                when 2
                                                                  if option1_name == product.selected_or_first_available_variant.option1 and variants_option2_arr[forloop.index0] == value and variants_available_arr[forloop.index0]
                                                                    assign option_disabled = false
                                                                  endif
                                                                when 3
                                                                  if option1_name == product.selected_or_first_available_variant.option1 and variants_option2_arr[forloop.index0] == product.selected_or_first_available_variant.option2 and variants_option3_arr[forloop.index0] == value and variants_available_arr[forloop.index0]
                                                                    assign option_disabled = false
                                                                  endif
                                                              endcase
                                                            endfor
                                                          -%}
                                                          <option class="input--{{ settings.typography_preset }}" value="{{ value | escape }}" {% if option.selected_value == value %}selected="selected"{% endif %}>
                                                            {% if option_disabled -%}
                                                              {{- 'products.product.value_unavailable' | t: option_value: value -}}
                                                            {%- else -%}
                                                              {{- value -}}
                                                            {%- endif %}
                                                          </option>
                                                        {%- endfor -%}
                                                      </select>
                                                      <div class="select-icon ">
                                                        {% if weight_accordion_title == 'regular' %}
                                                                {% render 'icon', icon: 'close-chevron-regular' %}
                                                        {% elsif weight_accordion_title == 'medium' %}
                                                                {% render 'icon', icon: 'close-chevron-medium' %}
                                                        {% elsif weight_accordion_title == 'bold' %}
                                                                {% render 'icon', icon: 'close-chevron-bold' %}
                                                        {% endif %}
                                                    </div>
                                                    </div>
                                                  </div>
                                                {% endif %}
                                                
                                              {% endfor %}
                                              <script type="application/json">
                                                {{ product.variants | json }}
                                              </script>
                                            </variant-selects>
                                    {%- endunless -%}
                                    <noscript class="product-form__noscript-wrapper-{{ section.id }}">
                                      <div class="product-form__input{% if product.has_only_default_variant %} hidden{% endif %}">
                                        <label class="form__label label--{{ settings.typography_preset }} {% if settings.dim_text %}dim{% endif %}" for="Variants-{{ section.id }}">{{ 'products.product.product_variants' | t }}</label>
                                        <div class="select">
                                          <select name="id" data-productid="{{ product.id }}" id="Variants-{{ section.id }}" class="select__select input--{{ settings.typography_preset }}" form="{{ product_form_id }}">
                                            {%- for variant in product.variants -%}
                                              <option
                                                class="input--{{ settings.typography_preset }}"
                                                {% if variant == product.selected_or_first_available_variant %}selected="selected"{% endif %}
                                                {% if variant.available == false %}disabled{% endif %}
                                                value="{{ variant.id }}"
                                              >
                                                {{ variant.title }}
                                                {%- if variant.available == false %} - {{ 'products.product.sold_out' | t }}{% endif %}
                                                - {{ variant.price | money | strip_html }}
                                              </option>
                                            {%- endfor -%}
                                          </select>
                                          
                                        </div>
                                      </div>
                                    </noscript>
                                  </div>
                                {%- when 'buy_buttons' -%}
