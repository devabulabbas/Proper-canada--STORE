{% style %}
#shopify-section-{{ section.id }} {
    {% if section.settings.sticky_header == 'always_stick' %}
        position: sticky;
        top: -0.5px;
    {% else %}
        position: relative;
    {% endif %}
    z-index: 20;
    {% if settings.allow_transparent_navigation %}
        background-color: transparent;
    {% endif %}
    --bottom-menu-alignment: {{ section.settings.menu_horizontal_alignment }};
    --header-alignment: {{ section.settings.block_alignment }};
    --padding: {{ section.settings.section_paddings }}px;
    {% if section.settings.color_type == 'invert' %}
      --layout-text-color:
        {{ settings.layout_background_color.red }},
        {{ settings.layout_background_color.green }},
        {{ settings.layout_background_color.blue }};
      --layout-background-color:
        {{ settings.layout_text_color.red }},
        {{ settings.layout_text_color.green }}, 
        {{ settings.layout_text_color.blue }};
      {% endif %}
      {% if section.settings.color_type == 'custom' %}
      --layout-text-color:
        {{ section.settings.text_color.red }},
        {{ section.settings.text_color.green }}, 
        {{ section.settings.text_color.blue }};
      --layout-background-color:
        {{ section.settings.background_color.red }},
        {{ section.settings.background_color.green }}, 
        {{ section.settings.background_color.blue }};
    {% endif %}
    }
    @media screen and (max-width: 920px) {
        #shopify-section-{{ section.id }} {
            display: none;
        }
    }
{% endstyle %}
 {% liquid
    assign blocksSize = section.blocks.size
    for block in section.blocks
        if block.type == 'mega_menu'
        assign megaMenuBlocks = section.blocks | where: 'type', "mega_menu"
        assign blocksSize = section.blocks.size | minus: megaMenuBlocks.size
        endif
    endfor
%}
{%- liquid
    assign blocks = section.blocks 
  -%}

    <{% if section.settings.sticky_header == 'stick_on_scroll' %}sticky-secondary-header{% else %}div{% endif %} class="header-section secondary-header-section {{ section.settings.sticky_header }} {% if settings.allow_transparent_navigation %}transparent-header{% endif %}">
    <header class="header
                header--{{ settings.white_space }}
                section-padding--{{ settings.white_space }} 
                {% if section.settings.sticky_header == 'disable' %}header--disable-sticky{% endif %}
                {% if section.settings.sticky_header == 'always_stick' %}header--sticky{% endif %}
                {% if settings.include_header_footer %}{% if settings.max_page_width == '1400' or settings.max_page_width == '1200' %}page-width {% endif %} page-width--{{ settings.max_page_width }}{% endif %}">
        <div class="header__wrapper {% if section.settings.show_bottom_line %}header__wrapper--line{% endif %}">
            {% if blocks.size > 0 %}
                <div class="header__content {% if section.settings.block_alignment == 'stretched' %}header__content--stretched{% endif %}">
                    {%- for block in blocks -%}
                        {%- case block.type -%}
                            {%- when 'logo' -%}
                                {%- render 'logo', logo_settings: block.settings, block_item: block -%}
                            {%- when 'main_menu' -%}
                                {% liquid
                                    if block.settings.menu == blank 
                                        assign blocksSize = blocksSize | minus : 1
                                    endif
                                %}
                                {% if block.settings.menu != blank %}
                                    {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu, block_item: block, type: block.type, for_loop: forloop, blocks: section.blocks -%}
                                {% endif %}
                            {%- when 'miscellaneous' -%}
                                {%- render 'miscellaneous', miscellaneous_settings: block.settings, block: block, blocks: section.blocks -%}
                            {%- when 'burger_menu' -%}
                                {%- render 'burger-menu', burger-menu_settings: block.settings, block_item: block -%}
                            {%- when 'cart' -%}
                                {% liquid
                                    if settings.disable_shopping_functionality == true
                                        assign blocksSize = blocksSize | minus : 1
                                    endif
                                %}
                                {%- if settings.disable_shopping_functionality == false -%}
                                    {%- render 'cart', cart_settings: block.settings, block: block -%}
                                {%- endif -%}
                            {%- when 'account' -%}
                                {%- render 'account', account_settings: block.settings, block_item: block -%}
                            {%- when 'search' -%}
                                {%- render 'search', search_settings: block.settings, type: block.type, block_item: block -%}
                            {%- when 'social_media' -%}
                                {% liquid
                                    if settings.instagram_link == blank and settings.twitter_link == blank and settings.tiktok_link == blank and settings.snapchat_link == blank and settings.pinterest_link == blank and settings.tumblr_link == blank and settings.facebook_link == blank and settings.youtube_link == blank and settings.vimeo_link == blank and settings.reddit_link == blank and settings.trustpilot_link == blank and settings.tripadvisor_link == blank and settings.telegram_link == blank 
                                        assign blocksSize = blocksSize | minus : 1
                                    endif
                                %}
                                {%- render 'social-media', social-media_settings: block.settings, block_item: block, section_settings: section.settings -%}
                            {%- when 'country_and_language' -%}
                                {% liquid
                                    if localization.available_countries.size == 1 and localization.available_languages.size == 1
                                        assign blocksSize = blocksSize | minus : 1
                                    endif
                                %}
                                {%- render 'country-and-language', country-and-language_settings: block.settings, type: block.type, block_item: block -%}
                            {%- when 'secondary_menu' -%}
                                {% liquid
                                    if block.settings.menu == blank 
                                        assign blocksSize = blocksSize | minus : 1
                                    endif
                                %}
                                {% if block.settings.menu != blank %}
                                    {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu, block_item: block, type: block.type -%}
                                {% endif %}
                            {%- when 'empty_space' -%}
                                <div class="empty-space"></div>
                            {%- when 'contacts' -%}
                                {%- render 'contacts', contacts_settings: block.settings, block_item: block -%}
                            {%- when 'buttons' -%}
                                {% liquid
                                    if block.settings.button_label_1 == blank and block.settings.button_label_2 == blank
                                        assign blocksSize = blocksSize | minus : 1
                                    endif
                                %}
                                {% if block.settings.button_label_1 != blank or block.settings.button_label_2 != blank %}
                                    <div class="navigation-buttons navigation-buttons--{{ block.id }}" data-block-index="{{ forloop.index0 }}" {{ block.shopify_attributes }}>
                                        <div class="navigation-buttons__wrapper">
                                            {%- render 'button-with-icon', button_settings: block.settings, button_label: block.settings.button_label_1, button_link: block.settings.button_link_1, button_style: block.settings.button_style_1, target_blank: block.settings.target_blank_1, icon: block.settings.icon_1, block: block -%}
                                            {%- render 'button-with-icon', button_settings: block.settings, button_label: block.settings.button_label_2, button_link: block.settings.button_link_2, button_style: block.settings.button_style_2, target_blank: block.settings.target_blank_2, icon: block.settings.icon_2, block: block -%}
                                        </div>
                                    </div>
                                {% endif %}
                            {%- when 'custom_liquid' -%}
                                {%- render 'custom-liquid', block: block -%}
                        {%- endcase -%}
                    {%- endfor -%}
                </div>
            {% endif %}
            {%- if section.settings.menu != blank -%}
            <div class="bottom-menu bottom-menu--{% if blocks.size > 0 %}padding{% endif %}">

            {%- liquid
                assign enable_mega_menu = false
            
                assign link_title_handle = link.title | handle
        
                for block in blocks
                    assign menu_heading_handle = block.settings.menu_heading | handle
                    if menu_heading_handle == link_title_handle
                    assign enable_mega_menu = true
                    assign mega_menu_block = block
                    assign mega_menu_block_id = block.id
                    break
                    endif
                endfor
        
            -%}
                {%- render 'menu', menu_settings: section.settings, menu_settings_id: section.settings.menu, blocks: section.blocks -%}
            </div>
            {%- endif -%}
        </div>
    </header>
<{% if section.settings.sticky_header == 'stick_on_scroll' %}/sticky-secondary-header{% else %}/div{% endif %}>

{% style %}
.header__content {
    --blocks-amount: {{ blocksSize }};
