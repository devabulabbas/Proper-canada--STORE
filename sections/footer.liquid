{{ 'menus.css' | asset_url | stylesheet_tag }}
{{ 'back-to-top.css' | asset_url | stylesheet_tag }}
{{ 'text.css' | asset_url | stylesheet_tag }}

{% style %}
    #shopify-section-{{ section.id }} {
        position: relative;
        --grid-columns: {{ section.settings.grid_columns }};
        {% if section.settings.color_type == 'invert' %}
        --layout-text-color:
            {{ settings.layout_background_color.red }},
            {{ settings.layout_background_color.green }},
            {{ settings.layout_background_color.blue }};
        --layout-background-color:
            {{ settings.layout_text_color.red }},
            {{ settings.layout_text_color.green }}, 
            {{ settings.layout_text_color.blue }};
        {% endif %}
        {% if section.settings.color_type == 'custom' %}
        --layout-text-color:
            {{ section.settings.text_color.red }},
            {{ section.settings.text_color.green }}, 
            {{ section.settings.text_color.blue }};
        --layout-background-color:
            {{ section.settings.background_color.red }},
            {{ section.settings.background_color.green }}, 
            {{ section.settings.background_color.blue }};
        {% endif %}
        --text-vertical-alignment: {{ block.settings.text_vertical_alignment }};
        --text-horizontal-alignment: {{ block.settings.text_horizontal_alignment }};
        --padding-top: {{ section.settings.desktop_padding_top }}px;
        --padding-bottom: {{ section.settings.desktop_padding_bottom }}px;
    }
    .back-to-top-container--minimal_uppercase .back-to-top__icon {
        height: {{ 24 | times: settings.desktop_font_scale | divided_by: 100.0 | round | at_least: 16 }}px;
    }
    .back-to-top-container--minimal_basic .back-to-top__icon,
    .back-to-top-container--classic .back-to-top__icon,
    .back-to-top-container--avant_garde .back-to-top__icon,
    .back-to-top-container--contrast .back-to-top__icon {
        height: {{ 16 | times: settings.desktop_font_scale | divided_by: 100.0 | round | at_least: 16 }}px;
    }
    .back-to-top-container--minimal_heavy .back-to-top__icon,
    .back-to-top-container--brutalist .back-to-top__icon {
        height: {{ 28 | times: settings.desktop_font_scale | divided_by: 100.0 | round | at_least: 16 }}px;
    }
    .back-to-top-container--duet .back-to-top__icon {
        height: {{ 26 | times: settings.desktop_font_scale | divided_by: 100.0 | round | at_least: 16 }}px;
    }
    @media screen and (max-width: 1024px) {
        .back-to-top-container--minimal_basic .back-to-top__icon,
        .back-to-top-container--classic .back-to-top__icon,
        .back-to-top-container--avant_garde .back-to-top__icon,
        .back-to-top-container--contrast .back-to-top__icon {
            height: {{ 24 | times: settings.desktop_font_scale | divided_by: 100.0 | round | at_least: 16 }}px;
        }
    }
    @media screen and (max-width: 920px) {
        #shopify-section-{{ section.id }} {
            --padding-top: {{ section.settings.mobile_padding_top }}px;
            --padding-bottom: {{ section.settings.mobile_padding_bottom }}px;
        }
        .back-to-top-container--minimal_uppercase .back-to-top__icon,
        .back-to-top-container--minimal_basic .back-to-top__icon,
        .back-to-top-container--classic .back-to-top__icon,
        .back-to-top-container--avant_garde .back-to-top__icon,
        .back-to-top-container--contrast .back-to-top__icon {
            height: {{ 24 | times: settings.mobile_font_scale | divided_by: 100.0 | round | at_least: 16 }}px;
        }
        .back-to-top-container--minimal_heavy .back-to-top__icon,
        .back-to-top-container--brutalist .back-to-top__icon {
            height: {{ 28 | times: settings.mobile_font_scale | divided_by: 100.0 | round | at_least: 16 }}px;
        }
        .back-to-top-container--duet .back-to-top__icon {
            height: {{ 26 | times: settings.mobile_font_scale | divided_by: 100.0 | round | at_least: 16 }}px;
        }
    }
    #shopify-section-{{ section.id }} .overlapping-container-js {
        position: absolute;
        height: 100%;
        top: 0;
        bottom: 0;
        background-color: inherit;
        {% if section.settings.section_width == 'overlap' and settings.allow_transparent_navigation %}
            width: 100vw !important;
        {% endif %}
        {% if settings.navigation_layout == 'sidebar_based' and settings.white_space == 'compact' and section.settings.section_width == 'overlap' and settings.allow_transparent_navigation %}
            {% if settings.sidebar_template == 'main_xs' or settings.sidebar_template == 'main_xs_with_secondary' %}
                transform: translate(min(-12.5%, -200px));
            {% elsif settings.sidebar_template == 'main_s' or settings.sidebar_template == 'main_s_with_secondary' %}
                transform: translate(min(-18.75%, -200px));
            {% elsif settings.sidebar_template == 'main_m' or settings.sidebar_template == 'main_m_with_secondary' %}
                transform: translate(min(-25%, -200px));
            {% elsif settings.sidebar_template == 'main_l' %}
                transform: translate(min(-50%, -200px));
            {% endif %}
        {% endif %}
        {% if settings.navigation_layout == 'sidebar_based' and settings.white_space == 'spacious' and section.settings.section_width == 'overlap' and settings.allow_transparent_navigation %}
            {% if settings.sidebar_template == 'main_xs' or settings.sidebar_template == 'main_xs_with_secondary' %}
                transform: translate(min(-12.5%, -280px));
            {% elsif settings.sidebar_template == 'main_s' or settings.sidebar_template == 'main_s_with_secondary' %}
                transform: translate(min(-18.75%, -280px));
            {% elsif settings.sidebar_template == 'main_m' or settings.sidebar_template == 'main_m_with_secondary' %}
                transform: translate(min(-25%, -280px));
            {% elsif settings.sidebar_template == 'main_l' %}
                transform: translate(min(-50%, -280px));
            {% endif %}
        {% endif %}
    }
{% endstyle %}

{%- liquid
    assign blocks = section.blocks 
  -%}

<div class=" {% if settings.allow_transparent_navigation and section.settings.section_width == 'overlap' %}overlapping-section{% endif %} footer color-{{ section.settings.color_type }} {% if section.settings.color_type != 'default' %}no-default-color{% endif %}">
    <div class="section-padding--{{ settings.white_space }} container">
        <div class="footer__content overlapping-content-js {% if settings.show_lines %}footer__content--lines{% endif %} {% if section.settings.show_top_line %}footer__content--line{% endif %} footer__content--{{ settings.white_space }} footer__content--{{ section.settings.grid_columns }} {% if settings.include_header_footer %}{% if settings.max_page_width == '1400' or settings.max_page_width == '1200' %}page-width {% endif %} page-width--{{ settings.max_page_width }}{% endif %}"
            data-bg="{%- if section.settings.color_type == 'default' -%}
                {{ settings.layout_background_color.red }},
                {{ settings.layout_background_color.green }}, 
                {{ settings.layout_background_color.blue }};
              {%- endif -%}
              {%- if section.settings.color_type == 'invert' -%}
                  {{ settings.layout_text_color.red }},
                  {{ settings.layout_text_color.green }}, 
                  {{ settings.layout_text_color.blue }};
              {%- endif -%}
              {%- if section.settings.color_type == 'custom' -%}
                  {{ section.settings.background_color.red }},
                  {{ section.settings.background_color.green }}, 
                  {{ section.settings.background_color.blue }};
              {%- endif -%}"
            data-color="{%- if section.settings.color_type == 'default' -%}
              {{ settings.layout_text_color.red }},
              {{ settings.layout_text_color.green }}, 
              {{ settings.layout_text_color.blue }}
            {%- endif -%}
            {%- if section.settings.color_type == 'invert' -%}
                {{ settings.layout_background_color.red }},
                {{ settings.layout_background_color.green }}, 
                {{ settings.layout_background_color.blue }}
            {%- endif -%}
            {%- if section.settings.color_type == 'custom' -%}
                {{ section.settings.text_color.red }},
                {{ section.settings.text_color.green }}, 
                {{ section.settings.text_color.blue }}
            {%- endif -%}
            ">
            {%- for block in blocks -%}
                {%- case block.type -%}
                    {%- when 'text' -%}
                        {%- render 'text', text_settings: block.settings, type: block.type, block_item: block, for_loop: forloop -%}
                    {%- when 'search' -%}
                        {%- render 'search', search_settings: block.settings, type: block.type, block_item: block -%}
                    {%- when 'main_menu' -%}
                        {% if block.settings.menu != blank %}
                            {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu, block_item: block, type: block.type -%}
                        {% endif %}
                    {%- when 'secondary_menu' -%}
                        {% if block.settings.menu_1 != blank or block.settings.menu_2 != blank or block.settings.menu_3 != blank or block.settings.menu_4 != blank or block.settings.menu_5 != blank %}
                            <div class="menus-block {{ block.settings.menu_vertical_alignment }} menus-block--{{ section.settings.grid_columns }} menus-block--{{ block.settings.block_width }} menus-block--{{ block.settings.menu_alignment }}" style="--menus-vertical-alignment: {{ block.settings.menu_vertical_alignment }}; --menus-horizontal-alignment: {{ block.settings.menu_horizontal_alignment }};" {{ block.shopify_attributes }}>
                                {%- liquid 
                                assign menu_item_1 = 0 | times: 1
                                assign menu_item_2 = 0 | times: 1
                                assign menu_item_3 = 0 | times: 1
                                assign menu_item_4 = 0 | times: 1
                                assign menu_item_5 = 0 | times: 1
                                if block.settings.menu_1 != blank
                                assign menu_item_1 = 1 | times: 1
                                endif
                                if block.settings.menu_2 != blank 
                                assign menu_item_2 = 1 | times: 1
                                endif
                                if block.settings.menu_3 != blank 
                                assign menu_item_3 = 1 | times: 1
                                endif
                                if block.settings.menu_4 != blank 
                                assign menu_item_4 = 1 | times: 1
                                endif
                                if block.settings.menu_5 != blank 
                                assign menu_item_5 = 1 | times: 1
                                endif
                                assign menus_items = menu_item_1 | plus: menu_item_2 | plus: menu_item_3 | plus: menu_item_4 | plus: menu_item_5
                                -%}
                                <div class="menus menus--{{ block.settings.menu_horizontal_alignment }} menus--{{ block.settings.menu_view }}" 
                                    style="--menus-items: {{ menus_items }}">
                                    {% if block.settings.menu_1 != blank %}
                                        <div class="menus__item menus__item--{{ settings.white_space }}">
                                            {% if block.settings.menu_1_heading != blank %}
                                                <p class="menus__item-title menus__item-title--{{ settings.typography_preset }} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.menu_1_heading }}</p>
                                            {% endif %}
                                            {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu_1, block_item: block, type: block.type -%}
                                        </div>
                                    {% endif %}
                                    {% if block.settings.menu_2 != blank %}
                                        <div class="menus__item menus__item--{{ settings.white_space }}">
                                            {% if block.settings.menu_2_heading != blank %}
                                                <p class="menus__item-title menus__item-title--{{ settings.typography_preset }} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.menu_2_heading }}</p>
                                            {% endif %}
                                            {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu_2, block_item: block, type: block.type -%}
                                        </div>
                                    {% endif %}
                                    {% if block.settings.menu_3 != blank %}
                                        <div class="menus__item menus__item--{{ settings.white_space }}">
                                            {% if block.settings.menu_3_heading != blank %}
                                                <p class="menus__item-title menus__item-title--{{ settings.typography_preset }} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.menu_3_heading }}</p>
                                            {% endif %}
                                            {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu_3, block_item: block, type: block.type -%}
                                        </div>
                                    {% endif %}
                                    {% if block.settings.menu_4 != blank %}
                                        <div class="menus__item menus__item--{{ settings.white_space }}">
                                            {% if block.settings.menu_4_heading != blank %}
                                                <p class="menus__item-title menus__item-title--{{ settings.typography_preset }} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.menu_4_heading }}</p>
                                            {% endif %}
                                            {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu_4, block_item: block, type: block.type -%}
                                        </div>
                                    {% endif %}
                                    {% if block.settings.menu_5 != blank %}
                                        <div class="menus__item menus__item--{{ settings.white_space }}">
                                            {% if block.settings.menu_5_heading != blank %}
                                                <p class="menus__item-title menus__item-title--{{ settings.typography_preset }} subheading {% if settings.dim_text %}dim{% endif %}">{{ block.settings.menu_5_heading }}</p>
                                            {% endif %}
                                            {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu_5, block_item: block, type: block.type -%}
                                        </div>
                                    {% endif %}
                                </div>
                            </div>
                        {% endif %}
                    {%- when 'miscellaneous' -%}
                        {%- render 'miscellaneous', miscellaneous_settings: block.settings, type: block.type, block: block -%}
                    {%- when 'line' -%}
                        <div class="line-container line-container--{{ section.settings.grid_columns }} line-container--{{ block.settings.block_width }} {% if section.id contains 'footer' %}line-container--footer{% else %}line-container--{{ section.id }}{% endif %}" data-block-index="{{ forloop.index0 }}" {{ block.shopify_attributes }} style="--line-alignment: {{ block.settings.line_alignment }};">
                            <span class="line line--{% if section.settings.custom %}colors{% endif %}"></span>
                        </div>
                    {%- when 'newsletter' -%}
                        {%- render 'newsletter', newsletter_settings: block.settings, block_item: block, for_loop: forloop -%}
                    {%- when 'social_media' -%}
                        {%- render 'social-media', social-media_settings: block.settings, type: block.type, block_item: block, section_settings: section.settings -%}
                    {%- when 'legal' -%}
                        <div class="legal-container legal-container--{{ section.settings.grid_columns }} legal-container--{{ block.settings.legal_horizontal_alignment }} legal-container--{{ block.settings.block_width }}" style="--legal-vertical-alignment: {{ block.settings.legal_vertical_alignment }}; --legal-horizontal-alignment: {{ block.settings.legal_horizontal_alignment }};" {{ block.shopify_attributes }}>
                            <div class="legal">
                                <div class="footer__copyright footer__copyright--{{ settings.typography_preset }} footer__copyright--{% if section.settings.custom %}colors{% endif %}">
                                    <p class="copyright__content">Copyright &copy; {{ 'now' | date: "%Y" }},&nbsp {{ shop.name | link_to: routes.root_url }} </p>
                                    {% if block.settings.legal_content != blank %}
                                    <div class="copyright__content copyright__content--text richtext {% if settings.enable_moving_underline %}richtext-link--underline{% endif %}">{{ block.settings.legal_content }}</div>
                                    {% endif %}
                                    <p class="copyright__content">{{ powered_by_link }}</p>
                                </div>
                                {% if block.settings.enable_back_to_top %}
                                    {%- render 'back-to-top', back-to-top_settings: block.settings, block_item: block -%}
                                {% endif %}
                            </div>
                        </div>
                    {%- when 'image' -%}
                        {%- render 'image', image_settings: block.settings, type: block.type, block_item: block -%}
                    {%- when 'context_image' -%}
                        <div class="context-image-container context-image-container--{{ section.settings.grid_columns }} context-image-container--{{ block.settings.block_width }} context-image-container--{{ block.id }}" {{ block.shopify_attributes }}>
                            {%- render 'context-image', block_or_section: block, block: block -%}
                        </div>
                    {%- when 'empty_space' -%}
                        <div class="empty-space empty-space--{{ section.settings.grid_columns }} empty-space--{{ block.settings.block_width }}"></div>
                    {%- when 'payment' -%}
                        <div class="payment-block payment-block--{{ section.settings.grid_columns }} payment-block--{{ block.settings.payment_horizontal_alignment }} payment-block--{{ block.settings.block_width }}" style="--payment-vertical-alignment: {{ block.settings.payment_vertical_alignment }}; --payment-horizontal-alignment: {{ block.settings.payment_horizontal_alignment }};" {{ block.shopify_attributes }}>
                            <div class="payment payment--{{ settings.white_space }}">
                                {% if block.settings.payment_heading != blank %}
                                    <p class="payment__heading payment__heading--{{ settings.typography_preset }} subheading subheading--{% if section.settings.custom %}colors{% endif %} {% if settings.dim_text %}dim{% endif %}">{{ block.settings.payment_heading }}</p>
                                {% endif %}
                                    <ul class="payment__list" role="list">
                                    {%- for type in shop.enabled_payment_types -%}
                                        <li class="payment__list-item">
                                        {{ type | payment_type_svg_tag }}
                                        </li>
                                    {%- endfor -%}
                                    </ul>
                            </div>
                        </div>
                    {%- when 'country_and_language' -%}
                        {%- render 'country-and-language', country-and-language_settings: block.settings, type: block.type, block_item: block -%}
                    {%- when 'contacts' -%}
                        {%- render 'contacts', contacts_settings: block.settings, block_item: block -%}
                    {%- when 'back_to_top' -%}
                        {%- render 'back-to-top', back-to-top_settings: block.settings, block_item: block -%}
                    {%- when 'buttons' -%}
                        <div class="navigation-buttons navigation-buttons--{{ block.id }} navigation-buttons--{{ section.settings.grid_columns }} navigation-buttons--{{ block.settings.block_width }}" data-block-index="{{ forloop.index0 }}" {{ block.shopify_attributes }}>
                            <div class="navigation-buttons__wrapper">
                                {%- render 'button-with-icon', button_settings: block.settings, button_label: block.settings.button_label_1, button_link: block.settings.button_link_1, button_style: block.settings.button_style_1, target_blank: block.settings.target_blank_1, icon: block.settings.icon_1, block: block -%}
                                {%- render 'button-with-icon', button_settings: block.settings, button_label: block.settings.button_label_2, button_link: block.settings.button_link_2, button_style: block.settings.button_style_2, target_blank: block.settings.target_blank_2, icon: block.settings.icon_2, block: block -%}
                            </div>
                        </div>
                    {%- when 'follow_on_shop' -%}
                        {%- if shop.features.follow_on_shop? -%}
                            <div class="follow-on-shop follow-on-shop--{{ section.settings.grid_columns }} follow-on-shop--{{ block.settings.block_width }}" style="--follow-vertical-alignment: {{ block.settings.follow_on_shop_vertical_alignment }}; --follow-horizontal-alignment: {{ block.settings.follow_on_shop_horizontal_alignment }};" {{ block.shopify_attributes }}>
                                <div class="footer__follow-on-shop">
                                    {% comment %} TODO: enable theme-check once `login_button` is accepted as valid filter {% endcomment %}
                                    {% # theme-check-disable %}
                                    {{ shop | login_button: action: 'follow' }}
                                    {% # theme-check-enable %}
                                </div>
                            </div>
                        {%- endif -%}
                    {%- when 'custom_liquid' -%}
                        {%- render 'custom-liquid', block: block -%}
                {%- endcase -%}
            {%- endfor -%}
        </div>
    </div>
    <div class="overlapping-container-js"></div>
</div>

{% schema %}
{
    "name": "t:sections.footer.name",
    "tag": "section",
    "class": "shopify-section-footer",
    "settings": [
        
        {
           "type": "checkbox",
           "id": "show_top_line",
            "default": false,
            "label": "t:sections.footer.settings.show_top_line.label"
        },
        {
            "type": "radio",
            "id": "section_width",
            "options": [
                {
                    "value": "overlap",
                    "label": "t:sections.all.section_width.options__1.label"
                },
                {
                    "value": "full-width",
                    "label": "t:sections.all.section_width.options__2.label"
                }
            ],
            "default": "full-width",
            "label": "t:sections.all.section_width.label"
        },
        {
            "type": "select",
            "id": "grid_columns",
            "options": [
                {
                    "value": "1",
                    "label": "t:sections.navigation_sections.general.grid_columns.options__1.label"
                },
                {
                    "value": "2",
                    "label": "t:sections.navigation_sections.general.grid_columns.options__2.label"
                },
                {
                    "value": "3",
                    "label": "t:sections.navigation_sections.general.grid_columns.options__3.label"
                }
            ],
            "default": "2",
            "label": "t:sections.navigation_sections.general.grid_columns.label",
            "info": "t:sections.navigation_sections.general.grid_columns.info"
        },
        {
            "type": "paragraph",
            "content": "t:sections.navigation_sections.general.block_alignment.paragraph__1.content"
        },
        {
            "type": "header",
            "content": "t:sections.all.colors.name"
        },
        {
            "type": "radio",
            "id": "color_type",
            "options": [
                {
                    "value": "default",
                    "label": "t:sections.all.colors.color_type.options__1.label"
                },
                {
                    "value": "invert",
                    "label": "t:sections.all.colors.color_type.options__2.label"
                },
                {
                    "value": "custom",
                    "label": "t:sections.all.colors.color_type.options__3.label"
                }
            ],
            "default": "invert",
            "label": "t:sections.all.colors.color_type.label"
        },
        {
            "type": "color",
            "id": "background_color",
            "label": "t:sections.all.colors.background_color.label",
            "default": "#FFFFFF"
        }, 
        {
            "type": "color",
            "id": "text_color",
            "label": "t:sections.all.colors.text_color.label",
            "default": "#000000"
        },
        {
            "type": "header",
            "content": "t:sections.all.paddings.desktop_section_paddings.content"
        }, 
        {
            "type": "range",
            "id": "desktop_padding_top",
            "min": -160,
            "max": 160,
            "step": 4,
            "unit": "px",
            "default": 0,
            "label": "t:sections.all.paddings.padding_top.label"
        }, 
        {
            "type": "range",
            "id": "desktop_padding_bottom",
            "min": -160,
            "max": 160,
            "step": 4,
            "unit": "px",
            "default": 0,
            "label": "t:sections.all.paddings.padding_bottom.label"
        },
        {
            "type": "header",
            "content": "t:sections.all.paddings.mobile_section_paddings.content"
        }, 
        {
            "type": "range",
            "id": "mobile_padding_top",
            "min": -80,
            "max": 80,
            "step": 4,
            "unit": "px",
            "default": 0,
            "label": "t:sections.all.paddings.padding_top.label"
        }, 
        {
            "type": "range",
            "id": "mobile_padding_bottom",
            "min": -80,
            "max": 80,
            "step": 4,
            "unit": "px",
            "default": 0,
            "label": "t:sections.all.paddings.padding_bottom.label"
        }
    ],
    "blocks": [
        {
            "type": "text",
            "name": "t:sections.navigation_sections.blocks.text.name",
            "settings": [
                {
                    "type": "select",
                    "id": "block_width",
                    "options": [
                        {
                            "value": "short",
                            "label": "t:sections.navigation_sections.general.block_width.options__1.label"
                        },
                        {
                            "value": "medium",
                            "label": "t:sections.navigation_sections.general.block_width.options__2.label"
                        },
                        {
                            "value": "wide",
                            "label": "t:sections.navigation_sections.general.block_width.options__3.label"
                        }
                    ],
                    "default": "wide",
                    "label": "t:sections.navigation_sections.general.block_width.label"
                },
                {
                   "type": "text",
                   "id": "text_subheading",
                   "label": "t:sections.all.labels.subheading.label"
                },
                {
                    "type": "select",
                    "id": "subheading_size",
                    "label": "t:sections.all.size.subheading_size.label",
                    "options": [
                        {
                            "value": "default",
                            "label": "t:sections.all.size.options__1.label"
                        },
                        {
                            "value": "heading_s",
                            "label": "t:sections.all.size.options__8.label"
                        },
                        {
