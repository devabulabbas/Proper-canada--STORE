{{ 'main-sidebar.css' | asset_url | stylesheet_tag }}
{% style %}
    #shopify-section-{{ section.id }} {
        display: flex;
        {% if settings.sidebar_template contains 'main_xs' %}
            flex: 0 0 12.5%; 
        {% elsif settings.sidebar_template contains 'main_s' %}
            flex: 0 0 18.75%; 
        {% elsif settings.sidebar_template contains 'main_m' %}
            flex: 0 0 25%; 
        {% elsif settings.sidebar_template contains 'main_l' %}
            flex: 0 0 50%; 
        {% endif %}
        {% if settings.white_space == 'spacious' %}
            min-width: 280px; 
        {% else %}
            min-width: 200px; 
        {% endif %}
        height: 100vh;
        position: sticky;
        overflow-x: visible;
        top: 0;
        z-index: 20;
	    --main-sidebar-max-width: {{ section.settings.sidebar_width | divided_by: 100.0 }};
        --main-sidebar-background: {{ section.settings.background_color }};
        --top-padding: {{ section.settings.top_padding }}px;
        --bottom-padding: {{ section.settings.bottom_padding }}px;
        --main-sidebar-alignment: {{ section.settings.block_alignment }};
        --blocks-gap: {{ section.settings.space_between_blocks }}px;
        {% if section.settings.color_type == 'invert' %}
        --layout-text-color:
            {{ settings.layout_background_color.red }},
            {{ settings.layout_background_color.green }},
            {{ settings.layout_background_color.blue }};
        --layout-background-color:
            {{ settings.layout_text_color.red }},
            {{ settings.layout_text_color.green }}, 
            {{ settings.layout_text_color.blue }};
        {% endif %}
        {% if section.settings.color_type == 'custom' %}
        --layout-text-color:
            {{ section.settings.text_color.red }},
            {{ section.settings.text_color.green }}, 
            {{ section.settings.text_color.blue }};
        --layout-background-color:
            {{ section.settings.background_color.red }},
            {{ section.settings.background_color.green }}, 
            {{ section.settings.background_color.blue }};
        {% endif %}
    }
    #shopify-section-main-sidebar shopify-section {
        width: 100%;
        height: 100vh;
    }

@media screen and (max-width: 920px) {
    #shopify-section-{{ section.id }} {
        display: none;
    }
}
{% endstyle %}

{% liquid
    assign blocksSize = section.blocks.size
    assign blocks = section.blocks 
%}

<div class="main-sidebar 
            section-padding--{{ settings.white_space }}
            color-{{ section.settings.color_type }} {% if section.settings.color_type != 'default' %}no-default-color{% endif %}
            {% if section.settings.block_alignment == 'stretched' %}main-sidebar--stretched{% endif %} 
            main-sidebar--{{ settings.white_space }} 
            {% if section.settings.show_right_line %}main-sidebar--line{% endif %}
            {% if settings.allow_transparent_navigation and section.settings.color_type == 'default' %}transparent-sidebar{% endif %}">
        {%- for block in blocks -%}
            {%- case block.type -%}
                {%- when 'logo' -%}
                    {%- render 'logo', logo_settings: block.settings, block_item: block, for_loop: forloop -%} 
                {%- when 'main_menu' -%}
                    {% liquid
                        if block.settings.menu == blank 
                            assign blocksSize = blocksSize | minus : 1
                        endif
                    %}
                    {% if block.settings.menu != blank %}
                        {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu, block_item: block, type: block.type, for_loop: forloop -%}
                    {% endif %}
                {%- when 'secondary_menu' -%}
                    {% liquid
                        if block.settings.menu == blank 
                            assign blocksSize = blocksSize | minus : 1
                        endif
                    %}
                    {% if block.settings.menu != blank %}
                        {%- render 'menu', menu_settings: block.settings, menu_settings_id: block.settings.menu, block_item: block, for_loop: forloop, type: block.type -%}
                    {% endif %}
                {%- when 'burger_menu' -%}
                    {%- render 'burger-menu', burger-menu_settings: block.settings, block_item: block -%}
                {%- when 'cart' -%}
                    {% liquid
                        if settings.disable_shopping_functionality == true 
                            assign blocksSize = blocksSize | minus : 1
                        endif
                    %}
                    {%- if settings.disable_shopping_functionality == false -%}
                        {%- render 'cart', cart_settings: block.settings, block: block -%}
                    {%- endif -%}
                {%- when 'miscellaneous' -%}
                    {%- render 'miscellaneous', miscellaneous_settings: block.settings, type: block.type, for_loop: forloop, block: block -%}
                {%- when 'social_media' -%}
                    {% liquid
                        if settings.instagram_link == blank and settings.twitter_link == blank and settings.tiktok_link == blank and settings.snapchat_link == blank and settings.pinterest_link == blank and settings.tumblr_link == blank and settings.facebook_link == blank and settings.youtube_link == blank and settings.vimeo_link == blank and settings.reddit_link == blank and settings.trustpilot_link == blank and settings.tripadvisor_link == blank and settings.telegram_link == blank 
                            assign blocksSize = blocksSize | minus : 1
                        endif
                    %}
                    {%- render 'social-media', social-media_settings: block.settings, block_item: block -%}
                {%- when 'search' -%}
                    {%- render 'search', search_settings: block.settings, block_item: block, type: block.type -%}
                {%- when 'account' -%}
                    {%- render 'account', account_settings: block.settings, block_item: block -%}
                {%- when 'country_and_language' -%}
                    {% liquid
                        if localization.available_countries.size == 1 and localization.available_languages.size == 1
                            assign blocksSize = blocksSize | minus : 1
                        endif
                    %}
                    {%- render 'country-and-language', country-and-language_settings: block.settings, type: block.type, for_loop: forloop, block_item: block -%}
                {%- when 'back_to_top' -%}
                    {%- render 'back-to-top', back-to-top_settings: block.settings, block_item: block -%}
                {%- when 'line' -%}
                    {%- liquid 
                        assign top_padding_mobile_line = block.settings.top_padding | divided_by: 4
                        assign bottom_padding_mobile_line = block.settings.bottom_padding | divided_by: 4
                    -%}
                    <div class="line-container line-container--{{ section.id }} line-container--{{ block.id }} " 
                        data-block-index="{{ forloop.index0 }}" 
                        id="{{ block.id }}" style="--line-alignment: {{ block.settings.line_alignment }};" {{ block.shopify_attributes }}>
                        {% style %}
                            .line-container--{{ block.id }} {
                            --padding-top: {{ block.settings.top_padding }}px;
                            --padding-bottom: {{ block.settings.bottom_padding }}px;
                            }
                            @media screen and (max-width: 920px) {
                                .line-container--{{ block.id }} {
                                --padding-top: {{ top_padding_mobile_line }}px;
                                --padding-bottom: {{ bottom_padding_mobile_line }}px;
                                }
                            }
                        {% endstyle %} 
                        <span class="line {% if section.settings.color_type == 'custom' %}color-custom{% elsif section.settings.color_type == 'invert' %}color-invert{% endif %} {% if settings.allow_transparent_navigation and settings.navigation_layout == 'sidebar_based' and section.settings.color_type == 'default' %}scroll-color{% endif %}"></span>
                    </div> 
                {%- when 'newsletter' -%}
                    {%- render 'newsletter', newsletter_settings: block.settings, block_item: block, for_loop: forloop -%}
                {%- when 'contacts' -%}
                    {%- render 'contacts', contacts_settings: block.settings, block_item: block -%}
                {%- when 'buttons' -%}
                    {% liquid
                        if block.settings.button_label_1 == blank and block.settings.button_label_2 == blank
                            assign blocksSize = blocksSize | minus : 1
                        endif
                    %}
                    {% if block.settings.button_label_1 != blank or block.settings.button_label_2 != blank %}
                        <div class="navigation-buttons navigation-buttons--{{ block.id }}" data-block-index="{{ forloop.index0 }}" {{ block.shopify_attributes }}>
                            <div class="navigation-buttons__wrapper">
                                {%- render 'button-with-icon', button_settings: block.settings, button_label: block.settings.button_label_1, button_link: block.settings.button_link_1, button_style: block.settings.button_style_1, target_blank: block.settings.target_blank_1, icon: block.settings.icon_1, block: block -%}
                                {%- render 'button-with-icon', button_settings: block.settings, button_label: block.settings.button_label_2, button_link: block.settings.button_link_2, button_style: block.settings.button_style_2, target_blank: block.settings.target_blank_2, icon: block.settings.icon_2, block: block -%}
                            </div>
                        </div>
                    {% endif %}
                {%- when 'custom_liquid' -%}
                    <div class="body-secondary-text--{{ settings.typography_preset }}" {{ block.shopify_attributes }}>{{ block.settings.custom_liquid }}</div>
                {%- when 'context_image' -%}
                    {% liquid
                        if block.settings.context_image == blank 
                            assign blocksSize = blocksSize | minus : 1
                        endif
                    %}
                    <div class="context-image-container context-image-container--{{ block.id }}" {{ block.shopify_attributes }}>
                        {%- render 'context-image', block_or_section: block, block: block -%}
                    </div>
            {%- endcase -%}
        {%- endfor -%}
</div>

{% style %}
.main-sidebar {
    --blocks-amount: {{ blocksSize }};
}
{% endstyle %}

